{
  "name": "pdf-tailwind-forms",
  "version": "2.2.2",
  "description": "TypeScript Node.js library for generating PDFs with AcroForms and TailwindCSS support",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "pdf",
    "forms",
    "acroforms",
    "tailwindcss",
    "typescript",
    "pdf-lib",
    "pdf-generator",
    "form-builder",
    "interactive-pdf",
    "pdf-forms"
  ],
  "author": "Lou TIGROUDJA <lou.tigroudja07@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AbsolumFrAG/pdf-tailwind-forms.git"
  },
  "bugs": {
    "url": "https://github.com/AbsolumFrAG/pdf-tailwind-forms/issues"
  },
  "homepage": "https://github.com/AbsolumFrAG/pdf-tailwind-forms#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "jest": "^30.1.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "pdf-lib": "^1.17.1",
    "puppeteer": "^24.18.0"
  }
}
